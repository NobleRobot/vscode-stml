{
	"name": "STML theme",
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#2c2a31",
		"editor.foreground": "#cedbce",
		"activityBarBadge.background": "#67a3f0",
		"sideBarTitle.foreground": "#d6dbc6"
	},
	"tokenColors": [
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
			],
			"settings": { "foreground": "#67a3f0", "fontStyle": "bold" }
		},
		{
			"name": "Brackets",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#687485"
			}
		},


		{	"name": "Block", "scope": ["block entity.name.tag", "entity.name.tag.block"],
			"settings": { "foreground": "#85AA06", "fontStyle": "bold" }
		},
		{	"name": "Block attributes", "scope": ["block meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#6b7b33" }
		},
		{	"name": "Block values", "scope": ["block constant.character"],
			"settings": { "foreground": "#cbdf87" }
		},


		{	"name": "Text", "scope": ["text entity.name.tag", "entity.name.tag.text"],
			"settings": { "foreground": "#E9C000", "fontStyle": "bold" }
		},
		{	"name": "Text attributes", "scope": ["text entity.other.attribute-name"],
			"settings": { "foreground": "#918033" }
		},
		{	"name": "Text values", "scope": ["text constant.character"],
			"settings": { "foreground": "#dccc86" }
		},


		{	"name": "Image", "scope": ["image entity.name.tag", "entity.name.tag.image"],
			"settings": { "foreground": "#00B8E5", "fontStyle": "bold" }
		},
		{	"name": "Image attributes", "scope": ["image meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#2d7587" }
		},
		{	"name": "Image values", "scope": ["image constant.character"],
			"settings": { "foreground": "#84c9db" }
		},


		{	"name": "Rule", "scope": ["rule entity.name.tag", "entity.name.tag.rule"],
			"settings": { "foreground": "#C4208C", "fontStyle": "bold" }
		},
		{	"name": "Rule attributes", "scope": ["rule meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#924277" }
		},
		{	"name": "Rule values", "scope": ["rule constant.character"],
			"settings": { "foreground": "#e186c3" }
		},


		{	"name": "List", "scope": ["list entity.name.tag", "entity.name.tag.list"],
			"settings": { "foreground": "#20AC9A", "fontStyle": "bold" }
		},
		{	"name": "List attributes", "scope": ["list meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#36847a" }
		},
		{	"name": "List values", "scope": ["list constant.character"],
			"settings": { "foreground": "#87dfd4" }
		},


		{	"name": "Item", "scope": ["item entity.name.tag", "entity.name.tag.item"],
			"settings": { "foreground": "#ABA920", "fontStyle": "bold" }
		},
		{	"name": "Item attributes", "scope": ["item meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#8a8931" }
		},
		{	"name": "Item values", "scope": ["item constant.character"],
			"settings": { "foreground": "#e1df87" }
		},


		{	"name": "Span", "scope": ["span entity.name.tag", "entity.name.tag.span"],
			"settings": { "foreground": "#E77C38", "fontStyle": "bold" }
		},
		{	"name": "Span attributes", "scope": ["span meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#8f5735" }
		},
		{	"name": "Span values", "scope": ["span constant.character"],
			"settings": { "foreground": "#eab18e" }
		},


		{	"name": "Link", "scope": ["link entity.name.tag", "entity.name.tag.link"],
			"settings": { "foreground": "#2893FF", "fontStyle": "bold" }
		},
		{	"name": "Link attributes", "scope": ["link meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#396b9d" }
		},
		{	"name": "Link values", "scope": ["link constant.character"],
			"settings": { "foreground": "#85b4e4" }
		},


		{	"name": "Embed", "scope": ["embed entity.name.tag", "entity.name.tag.embed"],
			"settings": { "foreground": "#C5264B", "fontStyle": "bold" }
		},
		{	"name": "Embed attributes", "scope": ["embed meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#9c384f" }
		},
		{	"name": "Embed values", "scope": ["embed constant.character"],
			"settings": { "foreground": "#ea93a7" }
		},



		{	"name": "Style", "scope": ["style entity.name.tag", "entity.name.tag.style"],
			"settings": { "foreground": "#FF86CE", "fontStyle": "bold" }
		},
		{	"name": "Style attributes", "scope": ["style meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#a1467c" }
		},
		{	"name": "Style values", "scope": ["style constant.character"],
			"settings": { "foreground": "#edafd4" }
		},


		{	"name": "Date", "scope": ["date entity.name.tag", "entity.name.tag.date"],
			"settings": { "foreground": "#D0506A", "fontStyle": "bold" }
		},
		{	"name": "Date attributes", "scope": ["date meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#964a59" }
		},
		{	"name": "Date values", "scope": ["date constant.character"],
			"settings": { "foreground": "#f0a7b6" }
		},


		{	"name": "Check", "scope": ["check entity.name.tag", "entity.name.tag.check"],
			"settings": { "foreground": "#49B450", "fontStyle": "bold" }
		},
		{	"name": "Check attributes", "scope": ["check meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#38783b" }
		},
		{	"name": "Check values", "scope": ["check constant.character"],
			"settings": { "foreground": "#86e38c" }
		},


		{	"name": "Value", "scope": ["value entity.name.tag", "entity.name.tag.value"],
			"settings": { "foreground": "#FF6D67", "fontStyle": "bold" }
		},
		{	"name": "Value attributes", "scope": ["value meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#974d4a" }
		},
		{	"name": "Value values", "scope": ["value constant.character"],
			"settings": { "foreground": "#ff9e9a" }
		},


		{	"name": "Include", "scope": ["include entity.name.tag", "entity.name.tag.include"],
			"settings": { "foreground": "#A0B773", "fontStyle": "bold" }
		},
		{	"name": "Include attributes", "scope": ["include meta.attribute entity.other.attribute-name"],
			"settings": { "foreground": "#6c8639" }
		},
		{	"name": "Include values", "scope": ["include constant.character"],
			"settings": { "foreground": "#daeabb" }
		},


		{	"name": "Attribute id", "scope": ["meta.attribute.id constant.character.entity.string"],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{	"name": "Attribute style",
			"scope": [
				"meta.attribute.style constant.character.entity.string",
				"meta.attribute.hoverStyle constant.character.entity.string",
				"meta.attribute.visitedStyle constant.character.entity.string",
				"meta.attribute.clickStyle constant.character.entity.string",
			],
			"settings": {
				"foreground": "#edafd4",
				"fontStyle": "italic"
			}
		},


		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#88AA9B",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Attribute", "scope": [ "entity.other.attribute-name" ],
			"settings": { "foreground": "#5b82b1" }
		},
		{
			"name": "String", "scope": [ "string", "string.unquoted" ],
			"settings": { "foreground": "#ade1f5" }
		},
		{
			"name": "Value",
			"scope": [
				"string", "string.unquoted",
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ade1f5"
			}
		},








		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#9896ae"
			}
		},


		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},

		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},

	]
}
