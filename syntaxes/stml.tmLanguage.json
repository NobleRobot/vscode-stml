					"begin": "[ \t]+(orientation)(?![^=\\s])", "end": "(?=\\s*+[^=\\s])", "beginCaptures": { "0": {"name": "entity.other.attribute-name.stml"}}, "name": "meta.attribute.$1.stml",
					"patterns": [{
