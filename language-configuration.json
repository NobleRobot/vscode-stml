{
	"comments": {
		"blockComment": ["<<", ">>"]
	},
	"brackets": [
		// Normal tags
		// stml|sss|head|body|title|author|details|block|text|list|item|span|link|tip|language|date|check|value
		["<stml>", "</>"], ["<sss>", "</>"],
		["<head>", "</>"], ["<body>", "</>"],
		["<title>", "</>"], ["<author>", "</>"], ["<details>", "</>"],
		["<block>", "</>"], ["<text>", "</>"],
		["<list>", "</>"], ["<item>", "</>"], ["<span>", "</>"], ["<link>", "</>"],
		["<tip", "</>"],
		["<language>", "</>"],
		["<date>", "</>"], ["<check>", "</>"], ["<value>", "</>"],

		// Self-closing tags
		// soul|music|rule|image|embed|include|style|break|tab|space| ...
		["<soul", "/>"],
		["<music ", "/>"],
		["<rule", " />"], ["<image", "/>"], ["<embed", "/>"], ["<include", "/>"],
		["<style", "/>"],
		["<break", "/>"], ["<tab", "/>"],  ["<space", "/>"],
	],
	"autoClosingPairs": [
		[ "<<", " >>" ],

		// Normal tags
		// stml|sss|head|body|title|author|details|block|text|list|item|span|link|tip|language|date|check|value
		["<stml>", "</>"], ["<sss>", "</>"],
		["<head>", "</>"], ["<body>", "</>"],
		["<title>", "</>"], ["<author>", "</>"], ["<details>", "</>"],
		["<block>", "</>"], ["<text>", "</>"],
		["<list>", "</>"], ["<item>", "</>"], ["<span>", "</>"], ["<link>", "</>"],
		["<tip", "</>"],
		["<language>", "</>"],
		["<date>", "</>"], ["<check>", "</>"], ["<value>", "</>"],

		// Self-closing tags
		// soul|music|rule|image|embed|include|style|break|tab|space| ...
		["<soul", "/>"],
		["<music ", "/>"],
		["<rule", " />"], ["<image", "/>"], ["<embed", "/>"], ["<include", "/>"],
		["<style", "/>"],
		["<break", "/>"], ["<tab", "/>"],  ["<space", "/>"],

		// Self-closing <sss> document styles
		// ... stml|body|block|text|list|item|span|link|tip
		["<stml", "/>"],
		["<body", "/>"],
		["<block", "/>"],
		["<text", "/>"],
		["<list", "/>"], ["<item", "/>"], ["<span", "/>"], ["<link", "/>"],
		["<tip", "</>"],

		// Extras
		["(", ")"],
		["[", "]"],
		["{", "}"],
		// < >, //Angle brackets are NOT wanted here (they interfere with comments).
		["'", "'"],
		["\"", "\""]
	],
	"autoCloseBefore": " \n\t",
	"surroundingPairs": [
		["(", ")"],
		["[", "]"],
		["{", "}"],
		["<", ">"],
		["'", "'"],
		["\"", "\""],
	],
	"folding": {
		"markers": {
			"start": "^\\s*<<\\s?section\\s[^>\\n]*\\s?>>",
			"end": "^\\s*<<\\s?/\\s?>>"
		}
	},
	"wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
}